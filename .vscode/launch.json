    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/read-database.js",
            "args": ["train-network.json"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Query Lines",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/query-database.js",
            "args": ["getLines", "940GZZLUOXC"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Query Stations",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/query-database.js",
            "args": ["getStations", "0"]
        }
    ]
}